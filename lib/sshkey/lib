indigo_sshkey_list(){ 
  case $1 in
    "") set_endpoint vm/sshkey ;;
    active) set_endpoint vm/sshkey/active/status ;;
    *) throw indigo_sshkey: invalid argument $1;;
  esac
}
indigo_sshkey_create(){ throw not implemented; }
indigo_sshkey_retrieve(){ set_endpoint vm/sshkey/${1:?indigo_sshkey_retrieveargument needed!}; }
indigo_sshkey_update(){ throw not implemented; }
indigo_sshkey_destroy(){ throw not implemented; }


